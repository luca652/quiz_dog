<%= simple_form_for @venue do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.input :quizmaster %>
  <%= f.input :day_of_the_week %>
  <%= f.input :time %>
  <%= f.input :photo, as: :file %>
  <%= f.hidden_field :user_id, value: current_user.id %>
  <%= f.submit %>
<% end %>
